    "    'max_features': ['auto', 'log2', None],\n",
    "    'n_iter_no_change': [20],\n",
    "    'validation_fraction': [0.2],\n",
    "    'tol': [0.01]\n",
    "}\n",
    "\n",
    "gbc_clf = GridSearchCV(estimator=gbc,\n",
    "                       param_grid=param_grid,\n",
    "                       scoring='f1',\n",
    "                       cv=5,\n",
    "                       verbose=False,\n",
    "                       n_jobs=-1)\n",
