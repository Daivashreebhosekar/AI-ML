    "    'n_estimators': [600],\n",
    "    'subsample': [0.66, 0.75],\n",
    "    'learning_rate': [0.001, 0.01],\n",
    "    'max_depth': [3,4,5], \n",
    "    'min_samples_split': [5, 7, 10],\n",
