    "credit = df['CreditScore'].to_list()\n",
    "Q1 = np.percentile(credit, 25, interpolation='midpoint')\n",
    "Q3 = np.percentile(credit, 75, interpolation='midpoint')\n",
    "IQR = Q3 - Q1\n",
    "print(\"CreditScore outliers: Min:\", Q1 - 1.5 * IQR, \"Max:\", Q3 + 1.5 * IQR)"
   ]
  },
