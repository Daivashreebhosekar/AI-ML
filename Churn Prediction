    "def geography_conversion(df):\n",
    "    df['Geography'] = df['Geography'].map({\n",
    "    'Germany': 1,\n",
    "    'Spain': 0,\n",
    "    'France': 0\n",
    "    })\n",
    "    return df"
   ]
  },
